{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "lib": ["es2017", "es7", "es6", "dom"],
    "declaration": false,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["node"],
    "module": "commonjs",
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
